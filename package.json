{
  "name": "gitify-website",
  "description": "A static website based on Jekyll for Gitify.",
  "version": "0.0.1",
  "scripts": {
    "watch": "sass _scss/_base.scss:static/css/base.css --sourcemap=none --watch",
    "build": "sass _scss/_base.scss:static/css/base.css --sourcemap=none --style compressed",
    "start": "npm run watch & jekyll serve --watch"
  },
  "dependencies": {
    "bootstrap": "=4.0.0-alpha.3",
    "font-awesome": "=4.6.3"
  },
  "devDependencies": {}
}
